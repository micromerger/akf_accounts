 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div class=\"row section-break\">\n   \n   <div class=\"col-xs-12\">\n       <img class=\"img_width\" src=\"/assets/akf_accounts/image/akf_logo.png\">\n   </div>\n   <div class=\"col-xs-12\">\n      <h3 class=\"text-center\">\n         <div>ALKHIDMAT FOUNDATION PAKISTAN</div>\n         <div class=\"font-12\">Branch: Al Khidmat Foundation Pakistan</div>\n         <div class=\"font-12 text-underline\">Payment Report</div>\n      </h3>\n   </div>\n</div>\n<br>\n<div class=\"row section-break\">\n   <div class=\"col-xs-12 column-break\">\n      <div class=\"row text-line-height\">\n         <div class=\"col-xs-3 text-bold\">\n            Doc Id:\n         </div>\n         <div class=\"col-xs-6\">\n            {{doc.name}}\n         </div>\n      </div>\n      <div class=\"row text-line-height\" >\n         <div class=\"col-xs-3 text-bold\">\n            Party:\n         </div>\n         <div class=\"col-xs-6\">\n            {{doc.party}}\n         </div>\n      </div>\n      <div class=\"row text-line-height\" >\n         <div class=\"col-xs-3 text-bold\">\n            Payee:\n         </div>\n         <div class=\"col-xs-6\">\n            {{doc.party_name}}\n         </div>\n      </div>\n      <div class=\"row text-line-height\" >\n         <div class=\"col-xs-3 text-bold\">\n            Payment Date:\n         </div>\n         <div class=\"col-xs-6\">\n            {{doc.get_formatted(\"posting_date\")}}\n         </div>\n      </div>\n      <div class=\"row text-line-height\" >\n         <div class=\"col-xs-3 text-bold\">\n            Payment Account:\n         </div>\n         <div class=\"col-xs-6\">\n            {{doc.paid_from}}\n         </div>\n      </div>\n      <div class=\"row text-line-height\" >\n         <div class=\"col-xs-3 text-bold\">\n            Disbursement Date:\n         </div>\n         <div class=\"col-xs-6\">\n            -\n         </div>\n      </div>\n      <div class=\"row text-line-height\" >\n         <div class=\"col-xs-3 text-bold\">\n            Cheque/Ref No:\n         </div>\n         <div class=\"col-xs-6\">\n            -\n         </div>\n      </div>\n   </div>\n</div>\n<br>\n<div class=\"row section-break\">\n    {% if doc.apply_tax_withholding_amount %}\n    {% set tax_rate = frappe.db.get_value(\"Tax Withholding Rate\", {\"parent\": doc.tax_withholding_category},\"tax_withholding_rate\") %}\n    {% endif %}\n    <div class=\"col-xs-12\">\n        <table class=\"table mt-3 table-bordered\" style=\"font-size: 14px;\">\n            <thead>\n            <tr>\n               <th class=\"\">\n                  <b>SR.</b>\n               </th>\n               <th class=\"details\">\n                  <b>Account</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>Description</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>Province</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>VB Code</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>Section</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>Gross Amount</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>WHT Rate</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>WH Type</b>\n               </th>\n               <th class=\"text-center\">\n                  <b>Net Amount</b>\n               </th>\n            </tr>\n            </thead>\n        <tbody>\n            <tr>\n                <td class=\"text-center\"> 1 </td>\n                <td class=\"text-center\"> {{doc.paid_to}} </td>\n                <td class=\"text-center\"> - </td>\n                <td class=\"text-center\"> - </td>\n                <td class=\"text-center\"> {{doc.name}} </td>\n                <td class=\"text-center\"> - </td>\n                <td class=\"text-center\"> {{doc.paid_amount}} </td>\n                {% if doc.apply_tax_withholding_amount %}\n                    <td class=\"text-center\"> {{ tax_rate }} </td>\n                    {% for row in doc.taxes %}\n                    {% if loop.first %}\n                    <td class=\"text-center\"> {{row.charge_type}} </td>\n                    <td class=\"text-center\"> {{row.total}} </td>\n                    {% endif %}\n                    {% endfor %}\n                    \n                {% else %}\n                    <td class=\"text-center\"> - </td>\n                    <td class=\"text-center\"> - </td>\n                    <td class=\"text-center\"> {{doc.paid_amount}} </td>\n                {% endif %}\n            </tr>\n         </tbody>\n      </table>\n   </div>\n   \n   <div class=\"col-xs-12\">\n      <table class=\"table mt-3 table-bordered\" style=\"font-size: 14px;\">\n         <thead>\n         </thead>\n         <tbody>\n            <tr>\n                <td class=\"text-bold\" colspan=\"2\">Gross Payment Total: </td>\n               <td class=\"text-center\"> {{doc.paid_amount}} </td>\n            </tr>\n            <tr>\n                <td class=\"text-bold\" colspan=\"2\">Withholding Tax Final: </td>\n                {% if doc.apply_tax_withholding_amount %}\n                    {% for row in doc.taxes %}\n                    {% if loop.first %}\n                        <td class=\"text-center\"> {{row.tax_amount}} </td>\n                    {% endif %}\n                    {% endfor %}\n                {% else %}\n                <td class=\"text-center\"> - </td>\n                {% endif %}\n            </tr>\n            <tr>\n               <td class=\"text-bold\" colspan=\"2\">Withholding Tax Adjustable: </td>\n               <td class=\"text-center\"> - </td>\n            </tr>\n            <tr>\n                <td class=\"text-bold\" colspan=\"2\">Net Payment: </td>\n                {% if doc.apply_tax_withholding_amount %}\n                    {% for row in doc.taxes %}\n                    {% if loop.first %}\n                        <td class=\"text-center\"> {{row.total}} </td>\n                    {% endif %}\n                    {% endfor %}\n                {% else %}\n                <td class=\"text-center\"> {{doc.paid_amount}} </td>\n                {% endif %}\n            </tr>\n         </tbody>\n      </table>\n   </div>\n</div>\n\n{% if frappe.db.exists(\"GL Entry\", {\"is_cancelled\": 0, \"voucher_no\": doc.name}) %}\n    <div class=\"row section-break\">\n       <div class=\"col-xs-12\">\n          <p class=\"text-bold\">GL INFO</p>\n        <table class=\"table mt-3 table-bordered\" style=\"font-size: 14px;\">\n            <thead>\n                <tr>\n                   <th class=\"\">\n                      <b>SR.</b>\n                   </th>\n                   <th >\n                      <b>Account</b>\n                   </th>\n                   <th class=\"text-center\">\n                      <b>Debit</b>\n                   </th>\n                   <th class=\"text-center\">\n                      <b>Credit</b>\n                   </th>\n                </tr>\n            </thead>\n            <tbody>\n                {% set gl_vars = {\n                        'total_debit': 0, \n                        'total_credit': 0,\n                    }\n                %}\n                {% set count = {'idx':0} %}\n                {% for row in frappe.db.get_list(\"GL Entry\", filters={\"is_cancelled\": 0, \"voucher_no\": doc.name}, fields=[\"*\"]) %}\n                <tr>\n                  {% if count.update({'idx': count.idx + 1}) %} {% endif %}\n                   <td> {{count.idx}} </td>\n                   <td> {{row.account}} </td>\n                   <td class=\"text-center\"> {{frappe.utils.fmt_money(row.debit_in_account_currency, currency=doc.currency)}}</td>\n                   <td class=\"text-center\"> {{frappe.utils.fmt_money(row.credit_in_account_currency, currency=doc.currency)}}</td>\n                    {% if gl_vars.update({'total_debit': gl_vars.total_debit + row.debit_in_account_currency}) %} {% endif %}\n                    {% if gl_vars.update({'total_credit': gl_vars.total_credit + row.credit_in_account_currency}) %} {% endif %}\n                </tr>\n                {% endfor %}\n                <tr>\n                   <td class=\"text-bold\" colspan=\"2\">Total</td>\n                   <td class=\"text-center\">{{ frappe.utils.fmt_money(gl_vars.total_debit, currency=doc.currency) }}</td>\n                   <td class=\"text-center\">{{frappe.utils.fmt_money(gl_vars.total_credit, currency=doc.currency)}}</td>\n                </tr>\n             </tbody>\n        </table>\n    </div>\n    </div>\n{% endif %}",
